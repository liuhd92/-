/* pages/daizhifu/daizhifu.wxss */
#homeMap,.fake-map {
    width: 100%;
    position: relative;
    height: calc(100vh - 559rpx);
}

.iphone-x #homeMap,.iphone-x .fake-map {
    height: calc(100vh - 605rpx);
}

.list-wrap {
    z-index: 2;
    padding-bottom: 30rpx;
}

.orderCountBg {
    width: 300rpx;
    height: 74rpx;
    position: absolute;
    top: 4%;
    left: 50%;
    transform: translateX(-50%);
}

.orderCountBtn {
    width: 300rpx;
    height: 72rpx;
}

.orderCount {
    width: 300rpx;
    height: 72rpx;
    color: #333333;
    background: #fff;
    border-radius: 150px;
    font-size: 24rpx;
    position: absolute;
    top: 3.9%;
    left: 50%;
    transform: translateX(-50%);
    display: flex;
    align-items: center;
    justify-content: center;
}

.orderText {
    margin-right: 10rpx;
    white-space: pre;
}

.orderArrow {
    margin-top: 2rpx;
    margin-left: -4rpx;
}

.calloutContainer {
    position: absolute;
    top: 40%;
    left: 50%;
    transform: translateX(-50%);
    width: 750rpx;
    height: 80rpx;
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: center;
}

.calloutBlock {
    height: 60rpx;
    border-radius: 30rpx;
    background-color: #000;
    box-shadow: 0 2px 4px 0 rgba(0,0,0,0.20);
    opacity: 0.75;
    padding: 0 24rpx 0 32rpx;
    color: #fff;
    font-size: 28rpx;
    line-height: 60rpx;
}

.centerCallout {
    color: #fff;
    display: inline-block;
    font-size: 28rpx;
    white-space: pre;
    line-height: 60rpx;
}

.calloutArrow {
    width: 30rpx;
    height: 20rpx;
    margin-top: -2px;
}

.centerPoint {
    position: absolute;
    top: 50%;
    left: 50%;
    transform: translateX(-50%);
    width: 40rpx;
    height: 56rpx;
}

.locateIcon {
    position: absolute;
    right: 20rpx;
    bottom: 40rpx;
    width: 70rpx;
    height: 70rpx;
}

.deleteAddr {
    width: 210rpx;
    height: 78rpx;
    position: absolute;
    left: 0;
    bottom: 40rpx;
}

.deleteAddrBtn {
    width: 210rpx;
    height: 78rpx;
}

.leftArrow {
    width: 10rpx;
    height: 16rpx;
}

.deleteText {
    margin-left: 20rpx;
}

.layer {
    z-index: 1000;
    position: fixed;
    left: 0;
    top: 0;
    background-color: #000;
    width: 100%;
    height: 100%;
    opacity: 0;
    transition: all 0.3s;
}

.containerShow {
    opacity: 0.5;
}

.coupon {
    z-index: 1001;
    width: 640rpx;
    position: fixed;
    left: 50%;
    top: -50%;
    transform: translate(-50%,-50%);
    transition: all 0.3s;
}

.modalHeadContainer {
    height: 320rpx;
    margin-bottom: -4rpx;
}

.modalHead {
    width: 640rpx;
    height: 320rpx;
    background-color: transparent;
}

.modalHeadDefault {
    background-color: #d8d8d8;
}

.modalBody {
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: center;
    padding: 30rpx 0;
}

.couponList {
    max-height: 518rpx;
    overflow: scroll;
}

.couponBlock {
    background: #ffffff;
    width: 580rpx;
    height: 160rpx;
    margin-bottom: 15rpx;
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: center;
    position: relative;
    color: #333333;
    border-radius: 10rpx;
}

.coupon-dot {
    display: block;
    height: 26rpx;
    width: 26rpx;
    border-radius: 50%;
}

.coupon-dot.left {
    position: absolute;
    top: 50%;
    margin-top: -13rpx;
    left: -13rpx;
}

.coupon-dot.right {
    position: absolute;
    top: 50%;
    margin-top: -13rpx;
    right: -13rpx;
}

.couponBg {
    position: absolute;
    top: 0;
    left: 0;
    width: 580rpx;
    height: 160rpx;
}

.couponLine {
    width: 500rpx;
    display: flex;
    justify-content: space-between;
    align-items: center;
}

.couponUp {
    position: absolute;
    top: 10rpx;
    left: 40rpx;
    font-size: 40rpx;
    line-height: 40rpx;
}

.couponTitle {
    color: #FF4136;
    overflow: hidden;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-box-orient: vertical;
    -webkit-line-clamp: 1;
    line-height: 50rpx;
}

.couponMoney {
    color: #FF4136;
    display: flex;
    align-items: flex-end;
}

.moneyUnit {
    margin-bottom: 6rpx;
}

.moneyValue {
    font-size: 80rpx;
    line-height: 80rpx;
}

.moneyDecimal {
    font-size: 36rpx;
    margin-bottom: 4rpx;
}

.couponBtm {
    position: absolute;
    left: 40rpx;
    bottom: 40rpx;
    font-size: 24rpx;
    line-height: 24rpx;
}

.enterBtn {
    width: 420rpx;
    height: 70rpx;
    margin-top: 15rpx;
    border-radius: 4rpx;
    font-size: 32rpx;
    display: flex;
    justify-content: center;
    align-items: center;
}

.modalFooter {
    margin-top: 30rpx;
    width: 640rpx;
    height: 80rpx;
    display: flex;
    justify-content: center;
}

.closeBtn {
    width: 80rpx;
    height: 80rpx;
}

.rightArrow {
    width: 10rpx;
    height: 16rpx;
}

.suspend-marketing {
    position: absolute;
    right: 20rpx;
    bottom: 170rpx;
    width: 100rpx;
    height: 130rpx;
}

.top-marketing {
    position: absolute;
    left: calc((50% - 365rpx));
    top: 10rpx;
    width: 730rpx;
    height: 110rpx;
    display: flex;
    align-items: center;
    justify-content: flex-start;
    border-radius: 3rpx;
    z-index: 10;
}

.top-marketing-background {
    position: absolute;
    width: 100%;
    height: 100%;
    z-index: -1;
}

.top-marketing-icon {
    width: 60rpx;
    height: 60rpx;
    margin-left: 30rpx;
}

.top-marketing-content {
    width: calc(100% - 168rpx);
    font-size: 28rpx;
    font-weight: bold;
    color: #666666;
    margin-left: 10rpx;
    overflow: hidden;
    text-overflow: ellipsis;
}

.top-marketing-arrow {
    width: 10rpx;
    height: 16rpx;
    position: absolute;
    top: 47rpx;
    right: 38rpx;
}

.infoWrapper {
    width: 100%;
    text-align: center;
}

.infoContainer {
    background-color: #fff;
    border-radius: 3rpx;
    color: #333333;
    font-size: 28rpx;
}

.addrBlock {
    position: relative;
    display: block;
    /* min-height: 269rpx; */
}

.recommendAddr {
    background-color: #6E707C;
    height: 112rpx;
    position: absolute;
    top: 98rpx;
    left: 28rpx;
    padding: 0 0 0 20rpx;
    display: flex;
    justify-content: space-between;
    align-items: center;
}

.recommendAddr .rightBtn {
    padding: 0 30rpx;
    height: 100%;
    display: flex;
    align-items: center;
}

.recommendAddr::before {
    content: '';
    display: block;
    background: url("https://p1.meituan.net/paotui/jnves00wwxl.png");
    background-size: 100% 100%;
    width: 17rpx;
    height: 10rpx;
    position: absolute;
    top: -9rpx;
    left: 60rpx;
}

.leftAddr {
    max-width: 393rpx;
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: flex-start;
    color: #fff;
    font-size: 24rpx;
    line-height: 33rpx;
}

.recommend-sender text {
    display: inline-block;
    vertical-align: middle;
}

.recommend-sender text.name {
    max-width: 98rpx;
    text-overflow: ellipsis;
    white-space: nowrap;
    overflow: hidden;
    margin-right: 20rpx;
}

.recommend-sender {
    overflow: hidden;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-box-orient: vertical;
    -webkit-line-clamp: 1;
}

.deleteBtn {
    width: 16rpx;
    height: 16rpx;
}
